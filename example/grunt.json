{
  "meta": {
    "name": "javascript-route-matcher",
    "version": "0.1pre",
    "description": "A simple route matching / url building utility. Intended to be included as part of a larger routing library.",
    "homepage": "http://benalman.com/",
    "author": "\"Cowboy\" Ben Alman",
    "license": ["MIT", "GPL"],
    "copyright": "Copyright (c) 2011 \"Cowboy\" Ben Alman",
    "repository": "git://github.com/cowboy/javascript-route-matcher.git"
  },
  "build": {
    "src": ["sample.js"],
    "min": "sample.min.js",
    "max": "sample.max.js"
  },
  "lint": {
    "pre": true,
    "post": false
  },
  "banner": "// {{.label}} - v{{.version}} - {{dateFormat(now, 'm/d/yyyy')}}\n// {{.homeurl}}\n// {{.copyright}}; Licensed {{.license.join(', ')}}",
  "jshint": {
    "curly": true,
    "eqnull": true,
    "immed": true,
    "newcap": true,
    "noarg": true,
    "undef": true,
    "browser": true,
    "predef": ["jQuery"]
  },
  "uglify": {
    "mangle": {"except": ["$"]},
    "squeeze": {},
    "codegen": {}
  }
}
